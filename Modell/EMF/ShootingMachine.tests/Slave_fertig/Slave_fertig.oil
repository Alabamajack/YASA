#include "implementation.oil"

CPU ATMEL_AT91SAM7S256
{
	OS LEJOS_OSEK
	{
		STATUS = EXTENDED;
		STARTUPHOOK = FALSE;
		ERRORHOOK = FALSE;
		SHUTDOWNHOOK = FALSE;
		PRETASKHOOK = FALSE;
		POSTTASKHOOK = FALSE;
		USEGETSERVICEID = FALSE;
		USEPARAMETERACCESS = FALSE;
		USERESSCHEDULER = FALSE;
	};

	APPMODE LEGOSAR{};

	TASK Trigger
	{
		AUTOSTART = TRUE
		{
			APPMODE = LEGOSAR;
		};
		PRIORITY = 2;
		ACTIVATION = 1;
		SCHEDULE = FULL;
		STACKSIZE = 512;
		EVENT = RTE_Trigger_StartTrigger_GetValue_Event_In_EVENT;
		EVENT = RTE_Trigger_GetValue_Receiver_In_EVENT;
	};

	TASK StartTrigger
	{
		AUTOSTART = FALSE;
		PRIORITY = 4;
		ACTIVATION = 1;
		SCHEDULE = FULL;
		STACKSIZE = 512;
	};

	TASK SchussMotor
	{
		AUTOSTART = TRUE
		{
			APPMODE = LEGOSAR;
		};
		PRIORITY = 3;
		ACTIVATION = 1;
		SCHEDULE = FULL;
		STACKSIZE = 512;
		EVENT = RTE_SchussMotor_GetValue_Event_In_EVENT;
	};

	TASK InitHook
	{
		AUTOSTART = TRUE
		{
			APPMODE = LEGOSAR;
		};
		PRIORITY = 10;
		ACTIVATION = 1;
		SCHEDULE = FULL;
		STACKSIZE = 512;
	};

	TASK TASK_BT_INTERFACE_READER
	{
		AUTOSTART = TRUE
		{
			APPMODE = LEGOSAR;
		};
		PRIORITY = 7;
		ACTIVATION = 1;
		SCHEDULE = FULL;
		STACKSIZE = 512;
		EVENT = BT_HAS_RECEIVED_PACKAGE;
	};

	TASK TASK_BT_INTERFACE_WRITER
	{
		AUTOSTART = TRUE
		{
			APPMODE = LEGOSAR;
		};
		PRIORITY = 7;
		ACTIVATION = 1;
		SCHEDULE = FULL;
		STACKSIZE = 512;
		EVENT = RTE_Schussanlage_Trigger_GetValue_Event_In_EVENT;
	};

	TASK BT_IMPLIZIT_SLAVE
	{
		AUTOSTART = TRUE
		{
			APPMODE = LEGOSAR;
		};
		PRIORITY = 9;
		ACTIVATION = 1;
		SCHEDULE = FULL;
		STACKSIZE = 512;
		EVENT = BT_SEND_MY_MESSAGE;
	};

	TASK BT_IMPLIZIT_SLAVE2
	{
		AUTOSTART = TRUE
		{
			APPMODE = LEGOSAR;
		};
		PRIORITY = 7;
		ACTIVATION = 1;
		SCHEDULE = FULL;
		STACKSIZE = 512;
		EVENT = BT_IMPLIZIT_SLAVE2_EVENT;
	};

	ALARM StartTriggerAlarm
	{
		COUNTER = C_StartTriggerAlarm;
		ACTION = ACTIVATETASK
		{
			TASK = StartTrigger;
		};
		AUTOSTART = TRUE
		{
			ALARMTIME = 100;
			CYCLETIME = 100;
			APPMODE = LEGOSAR;
		};
	};

	COUNTER C_StartTriggerAlarm
	{
		MINCYCLE = 1;
		MAXALLOWEDVALUE = 100;
		TICKSPERBASE = 1;
	};

	EVENT BT_HAS_RECEIVED_PACKAGE
	{
		MASK = AUTO;
	};

	EVENT BT_SEND_MY_MESSAGE
	{
		MASK = AUTO;
	};

	EVENT RTE_Trigger_StartTrigger_GetValue_Event_In_EVENT
	{
		MASK = AUTO;
	};

	EVENT RTE_Trigger_GetValue_Receiver_In_EVENT
	{
		MASK = AUTO;
	};

	EVENT RTE_SchussMotor_GetValue_Event_In_EVENT
	{
		MASK = AUTO;
	};

	EVENT RTE_Schussanlage_Trigger_GetValue_Event_In_EVENT
	{
		MASK = AUTO;
	};

	EVENT BT_IMPLIZIT_SLAVE2_EVENT
	{
		MASK = AUTO;
	};

};